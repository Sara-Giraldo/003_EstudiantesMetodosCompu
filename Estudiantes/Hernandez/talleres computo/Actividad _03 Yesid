 if A[i, i] == 0:\n","            continue\n","\n","        A[i] = A[i] / A[i, i]\n","\n","        # Hacer ceros en la columna i\n","        for j in range(f):\n","            if j != i:\n","                A[j] -= A[j, i] * A[i]\n","\n","    return A\n","gauss_jordan(O)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"5WK0a_3Oy305","executionInfo":{"status":"ok","timestamp":1742763941856,"user_tz":300,"elapsed":17,"user":{"displayName":"CESAR YESID HERNANDEZ TARAZONA","userId":"17433149652691072505"}},"outputId":"68721503-b2f0-4d84-e658-be36fbc7a5bc"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["array([[1, 0, 0],\n","       [0, 1, 0],\n","       [0, 0, 1]])"]},"metadata":{},"execution_count":20}]}]}
